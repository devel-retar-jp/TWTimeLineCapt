//コメント入り。参考用です。これは使えません。
{
  //*******************************************//
  //Consumer API keys (API key)
  "twConsumerKey": "ConsumerKey",
  //Consumer API keys (API secret key)
  "twConsumerSecret": "Consumer API keys",
  //twAccess token & access token secret  (Access token)
  "twAccessToken": "Access token",
  //Access token & access token secret  (Access token secret)
  "twAccessSecret": "Access token secret",
  //*******************************************//

  //*******************************************//
  //出力ディレクトリ
  //実行Dirの下に作成し結果をすべて出力します。
  "twOutSubDir": "Outs",
  //*******************************************//

  //*******************************************//
  //取得回数  5000フォロワーにつき1回
  //https://developer.twitter.com/en/docs/accounts-and-users/follow-search-get-users/api-reference/get-friends-ids
  //0     - 5000 フォロワー => 1
  //5001  - 10000フォロワー => 2
  //10001 - 15000フォロワー => 3
  //15001 - 20000フォロワー => 4
  "twFriendsIdsCount": 2,
  //*******************************************//

  //*******************************************//
  //取得ID数 MAX 5000 最大値のままでOK
  "twFriendsIdsParm_count": 5000,
  //*******************************************//

  //*******************************************//
  //スクリーン名（他人のアカウントは動かないはず）
  "twFriendsIdsParm_screen_name": "@retar_jp",
  //*******************************************//

  //*******************************************//
  //カーソル位置 （いじらない）
  "twFriendsIdsParm_cursor": "",
  //*******************************************//

  //*******************************************//
  //出力ファイル（ID取得APIから取得するファイル）
  "twFriendsIdsOutFileName": "Ids.csv",
  //*******************************************//

  //*******************************************//
  //出力ファイル true/false
  //ファイル出力するかどうか（いじらない）
  "twFriendsIdsOutFileOut": true,
  //*******************************************//

  //*******************************************//
  //フォロワーとフォロー中　Followers / Friends
  //Followers => フォロワーの「ツイートと返信」
  //Friends => フォロワーの「ツイートと返信」
  "twFriendsIdsTypeOfFollows": "Friends",
  //*******************************************//

  //*******************************************//
  //フォロワーとフォロー　Net(true)/File(false) 取得 スイッチ
  //true  => ネットからデータを取得する
  //false => オフラインデータをそのまま使用する
  "twFriendsIdsGets": false,
  //*******************************************//

  //*******************************************//
  //タイムラインの取得数
  //要求しても取りこぼしがあるので2-3つ多めにとってくる。
  "twTimeLineCounts": 23,
  //*******************************************//

  //*******************************************//
  //タイムラインの取得 Sleep Twitter APIの制限(いじらない)
  "twTimeLineSleepTime": 1100,
  //*******************************************//

  //*******************************************//
  //失敗したユーザの一覧 鍵ユーザ、削除ユーザの一覧
  //フォローされているけど、BANされている
  //鍵をかけてタイムラインが読めないユーザ
  //真っ先にフォロー対象から外す
  "twTimeLineFailedUsersFile": "FailUserTimeRun.csv",
  //*******************************************//

  //*******************************************//
  //ユーザタイムランをCSVファイルに変換
  "twTimeLineUsersFile": "UserTimeRun.csv",
  //*******************************************//

  //*******************************************//
  //ユーザタイムランのJSON Dir
  //サブディレクトリ 
  //フォロー数 * タイムラインの取得数
  "twTimeLineUsersJSONDir": "UserTimeRunJSON",
  //*******************************************//

  //*******************************************//
  //ユーザタイムラン Net(true)/File(false) 取得 スイッチ
  //true  => ネットからデータを取得する
  //false => オフラインデータをそのまま使用する
  "twTimeLineGets": false,
  //*******************************************//

  //*******************************************//
  ///Tweets Latest Filter 20日以内にTweetしている
  //サブディレクトリ
  "twLatestFilter": "Analysis000Counter",
  //*******************************************//

  //*******************************************//
  ///Tweets Latest Filter 20日以内にTweetしている Counter
  "twLatestFilterCounter": 20,
  //*******************************************//

  //*******************************************//
  //Tweets Counter つぶやきが一定数以下のアカウントを抽出
  //サブディレクトリ
  "twAnalysisCounter": "Analysis001Counter",
  //*******************************************//

  //*******************************************//
  //Tweets Counter つぶやきが一定数以下のアカウントを抽出 Counter
  //twTimeLineCountsから2〜3引いた値
  "twAnalysisCounterCount": 20,
  //*******************************************//

  //*******************************************//
  //Tweets Counter 特定用語の抽出
  //サブディレクトリ
  "twAnalysisWord": "Analysis002Counter",
  //*******************************************//

  //*******************************************//
  //Tweets Counter twGetGrayWords つぶやきワード
  //毒語の正規表現
  "twGetGrayWords": [
    "無料",
    "お金",
    "情報",
    "簡単",
    "利益",
    "副業",
    "[Ll][Ii][Nn][Ee]",
    "ライン",
    "ビジネス",
    "ツ[ー-〜−]ル",
    "稼ぎ",
    "報酬",
    "完全",
    "ご覧",
    "御覧",
    "倍",
    "人生",
    "ノウハウ",
    "公式",
    "チャンス",
    "出会い",
    "配信",
    "自動",
    "連絡",
    "当社",
    "コンサルティング"
  ],
  //*******************************************//

  //*******************************************//
  //Tweets Counter twGetIpadicDir 
  //辞書ディレクトリ
  "twGetIpadicDir": "ipadic",
  //*******************************************//

  //*******************************************//
  //Tweets Counter 
  //特定用語の勘定
  "twAnalysisWordCounter": "Analysis003Counter",
  //*******************************************//

  //*******************************************//
  //Tweets Counter 特定用語の勘定ファイル
  "twWordCounter": "WordCounter.csv",
  //*******************************************//

  //*******************************************//
  //Tweets Counter 特定用語の名詞勘定ファイル
  //(機能追加していません。Excelで十分です)
  "twWordCounterNoun": "WordCounterNoun.csv",
  //*******************************************//

  //*******************************************//
  //入力 DIR    テキストマージプログラム拡張
  "sgINDIR": "IN",
  //*******************************************//

  //*******************************************//
  //出力 DIR    テキストマージプログラム拡張
  "sgOUTDIR": "OUT",
  //*******************************************//

  //*******************************************//
  //出力ファイル　独自部分SUFFIX    テキストマージプログラム拡張
  "sgORGSUFFIX": "_ORG.csv",
  //*******************************************//

  //*******************************************//
  //出力ファイル　共通ファイル    テキストマージプログラム拡張
  "sgCOMMONSUFFIX": "COMMON.csv",
  //*******************************************//

  //*******************************************//
  //出力ファイル　マージファイル    テキストマージプログラム拡張
  "sgMERGESUFFIX": "MERGE.csv"
  //*******************************************//
}
